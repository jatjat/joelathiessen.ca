  "scripts": {
    "start": "nodemon dist/backend.bundle.js -p 3000",
    "start:prod": "NODE_ENV=production PORT=80 nodemon dist/backend.bundle.js",
    "build":
      "webpack --mode production --config webpack/webpack.backend.js && webpack --mode production --config webpack/webpack.prod.js",
    "watch":
      "webpack-dev-server -w --mode development --inline --hot --port 8080 --progress --colors --config webpack/webpack.dev.js",
